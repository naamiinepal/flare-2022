            ],
            "env": {
                "CUDA_LAUNCH_BLOCKING": "1"
            },
            "justMyCode": false
        },
        {
            "name": "Debug Submission",
            "type": "python",
            "request": "launch",
            "program": "predict_c2f_scratch.py",
